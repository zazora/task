    "import numpy as np\n",
    "class Figure_S:\n",
    "\tdef _init_(self):\n",
    "\t\tpass\n",
    "\tdef circle(self,r):\n",
    "\t\treturn np.pi*pow(r,2)\n",
    "\tdef triangle(self,x,y,z):\n",
    "\t\treturn np.sum([x,y,z])/2 \n",
    "\n",
    "r,x,y,z=5,4,3,2\n",
    "print(Figure_S().circle(r))\n",
    "print(Figure_S().triangle(x,y,z))"
